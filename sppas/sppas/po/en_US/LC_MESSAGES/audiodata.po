msgid ":ERROR 2000: "
msgstr "No audio file defined."

msgid ":ERROR 2005: "
msgstr "Audio type error: not supported file format {extension}. "

msgid ":ERROR 2010: "
msgstr "{filename} opening, reading or writing error: {message}."

msgid ":ERROR 2015: "
msgstr "No data or corrupted data in the audio file {filename}."

msgid ":ERROR 2020: "
msgstr "{number} is not a right index of channel."

msgid ":ERROR 2025: "
msgstr "From {value1} to {value2} is not a proper interval."

msgid ":ERROR 2050: "
msgstr "No channel defined."

msgid ":ERROR 2060: "
msgstr "Channels have not the same sample width."

msgid ":ERROR 2061: "
msgstr "Channels have not the same frame rate."

msgid ":ERROR 2062: "
msgstr "Channels have not the same number of frames."

msgid ":ERROR 2070: "
msgstr "Invalid sample width. Expect 1, 2 or 4. Got {value}."

msgid ":ERROR 2080: "
msgstr "Invalid framerate. Got {value}."
