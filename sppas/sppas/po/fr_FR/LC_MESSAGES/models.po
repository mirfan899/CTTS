msgid ":ERROR 7500: "
msgstr "Le fichier {!s:s} des caractères qui ne sont pas encodés en UTF-8: {:s}."

msgid ":ERROR 7505: "
msgstr "Format de fichier inconnu: {!s:s}."

msgid ":ERROR 7510: "
msgstr "Le dossier {!s:s} ne contient aucun modèle d'un format de fichier connu."

msgid ":ERROR 7515: "
msgstr "Aucun modèle n'est trouvé ou le modèle est vide: {!s:s}."

msgid ":ERROR 7010: "
msgstr "Pour {data_name}, le type {expected_type} était attendu, au lieu du type {data_type}."

msgid ":ERROR 7110: "
msgstr "{got_value} n'est pas une valeur correcte pour un modèle n-gram. Elle doit être comprise entre {min_value} et {max_value}."

msgid ":ERROR 7120: "
msgstr "{got_value} n'est pas une valeur correcte pour la valeur minimale d'une occurrence, qui doit être au moins de {min_value}."

msgid ":ERROR 7130: "
msgstr "{got_name} n'est pas un nom de méthode connu pour l'estimation du repli."

msgid ":ERROR 7210: "
msgstr "Un fichier ARPA standard est requis. Erreur avec la ligne: {line}."
